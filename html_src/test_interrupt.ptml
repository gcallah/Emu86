            <h1>
            Intel
            </h1>
            <pre>
            <code>
         mov eax, 0
         int 22
         mov [0], eax
         mov ebx, 1

loop:    mov edx, eax
         mov eax, 0
         int 22
         mov [ebx], eax
         cmp [0], eax
         je done
         inc ebx
         jmp loop
done: mov ecx, 1
            </code>
