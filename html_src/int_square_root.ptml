        <title>
            int_square_root.asm
        </title>
    </head>
    <body>
        <div class="wrapper">
<!--include navbar.txt -->
            <div id="content">
            <h1>
            int_square_root.asm
            </h1>
            <p>
Declare a number
            </p>
            <pre>
            <code>
.data
    number DW 100

            </code>
            </pre>
            <p>
Calculate square root of the number
            </p>
            <pre>
            <code>
.text
    mov eax, [number]
    push ebx
    push ecx
    mov ebx, 0
WhileLE: mov ecx, ebx
         imul ecx, ebx
         cmp ecx, eax
         jnle EndWhileLE
         inc ebx
         jmp WhileLE
EndWhileLE: dec ebx
            mov eax, ebx
            pop ecx
            pop ebx
            </code>
            </pre>
            </div>
        </div>
    </body>
</html>
