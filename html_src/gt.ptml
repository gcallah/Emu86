    <body>
        <div class="wrapper">
<!--include navbar.txt -->
            <div id="content">
            <h1>
            gt.asm
            </h1>
            <pre>
            <code>
         mov eax, 0
         mov ebx, 16

            </code>
            </pre>
            <p>
compare eax and ebx and loop until eax greater than ebx
            </p>
            <pre>
            <code>
loop: cmp eax, ebx
         jg done
         inc eax
         jmp loop

            </code>
            </pre>
            <p>
when done, store ebx in ecx
            </p>
            <pre>
            <code>
done: mov ecx, ebx
         mov edx, 27
         and edx, 23
            </code>
            </pre>
            </div>
        </div>
    </body>
